.section {
  padding: 20rpx;
  font-size: 30rpx;
}
.notice,
.from-info {
  margin-bottom: 20rpx;
}
.from-info input {
  background-color: #EFEFEF;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-indent: 20rpx;
}
.questionnaire-list,
.questionnaire-item {
  margin-top: 40rpx;
}
.questionnaire-item radio-group {
  margin-top: 20rpx;
   padding-left: 20rpx; 
  padding-right: 20rpx;  
}
.questionnaire-item radio-group label {
  display: inline-block;
  width: 100%;
  margin: 20rpx 0; 
}
.questionnaire-item radio-group textarea {
  width: 90%;
  margin-top: 40rpx;
  padding: 40rpx;
  background-color: #EFEFEF;
}
.questionnaire-submit {
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}